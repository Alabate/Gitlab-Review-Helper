{
  "description": "Gitlab review helper",
  "manifest_version": 2,
  "name": "GitlabReviewHelper",
  "version": "1.0",
  "homepage_url": "https://github.com/Alabate/Gitlab-Review-Helper",
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["*://*/*/merge_requests/*"],
      "js": ["main.js"]
    }
  ],
  "permissions": ["storage"]
}
